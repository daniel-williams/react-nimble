{
  "name": "react-min",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=dev npm run webpack-dev-server",
    "remote": "cross-env NODE_ENV=remote npm run webpack-dev-server",
    "build:stag": "cross-env NODE_ENV=staging npm run webpack-build",
    "build:prod": "cross-env NODE_ENV=production npm run webpack-build",
    "webpack-build": "rimraf dist && webpack --config config/webpack.production.js --progress --profile --bail",
    "webpack-dev-server": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base client/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5",
    "html-webpack-plugin": "^2.30.1",
    "source-map-loader": "^0.2.2",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  }
}
